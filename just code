
from tkinter import Tk, Button, Label, StringVar, Frame , Canvas

#main window
class main_wind (Tk) :
    def __init__(self):
        super().__init__

    #window config
    self.title('Space Invaders')
    self.geometry('1600x1200+100+400')

    #button
    self.button = Button (self)
    self.button['command'] = self.button_clicked
    self.button.pack()

    #canvas
    self.canvas = Canvas (self)
    




main_wind.mainloop()
